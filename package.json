{
  "name": "theater",
  "version": "0.0.1",
  "main": "dist/theater.js",
  "scripts": {
    "css:compile": "node-sass --output-style expanded --source-maps true --source-map-contents --true --precision 6 scss/*.scss -o dist/css/",
    "css:lint": "stylelint --config .stylelintrc.json --syntax scss \"scss/*.scss\"",
    "css:watch": "node-sass --output-style expanded --source-maps true --source-map-contents --true --precision 6 -w scss/*.scss -o dist/css/",
    "js:build": "babel js/ --out-dir dist/js/",
    "js:lint": "eslint --config .eslintrc.json js/*.js --fix"
  },
  "repository": "https://github.com/mattonit/theater",
  "author": "Mattonit",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-block-scoped-functions": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.8.0",
    "gulp": "^3.9.1",
    "node-sass": "^4.9.4",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "uglify-js": "^3.4.9"
  }
}
